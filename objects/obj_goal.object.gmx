        <arguments>
          <argument>
            <kind>1</kind>
            <string>hspeed *= 0.8;
vspeed *= 0.8;

mid_x = (x + 10);
mid_y = (y + 10);

//spawning
if (keyboard_check(vk_shift) &amp;&amp; (place_meeting(x, y + 5, obj_spawn) or place_meeting(x, y + 5, obj_dirt_spawn) or place_meeting(x,y+3, obj_dirt_slant_spawn)) &amp;&amp; !place_meeting(x, y, obj_collision))
{
  instance_change(obj_mushroomman, true);
}

//spores

for (i = 0; i &lt; global.NUM_SPORES; i += 1)
{
  spore = global.spores[i];
  spore.hspeed += global.RANDOM_SCALE * ((2 * random(1))-1);
  spore.vspeed += global.RANDOM_SCALE * ((2 * random(1))-1);
  spore.hspeed += global.COHESION_SCALE * (mid_x - spore.x);
  spore.vspeed += global.COHESION_SCALE * (mid_y - spore.y);
  dist = abs(point_distance(mid_x, mid_y, spore.x, spore.y));
  if (abs(dist) &gt; MAX_DIST)
  {
    scaling = dist - MAX_DIST;
    xPart = (mid_x - spore.x) / dist;
    yPart = (mid_y - spore.y) / dist;
    spore.x += xPart * scaling;
    spore.y += yPart * scaling;
  }
}


//movement
if (keyboard_check(vk_up)) 
{
  vspeed -= 6;
  seeking_flag = false;
}

if (keyboard_check(vk_down)) 
{
  vspeed += 5;
  seeking_flag = false;
}

if (keyboard_check(vk_right)) 
{
  hspeed += 5;
  seeking_flag = false;
}

if (keyboard_check(vk_left)) 
{
  hspeed -= 5;
  seeking_flag = false;
}

if (keyboard_check_pressed(vk_tab))
{
  seeking_flag = true;
}

epsilon = 0.0001;

if (seeking_flag)
{
  move_towards_point(obj_lastspawn.x, obj_lastspawn.y, SWARM_MAX_SPEED+5);
  if (distance_to_point(obj_lastspawn.x, obj_lastspawn.y - 10) &lt; epsilon)
  {
    seeking_flag = false;
  }
}

hspeed = min(SWARM_MAX_SPEED, max(SWARM_MIN_SPEED, hspeed));
vspeed = min(SWARM_MAX_SPEED, max(SWARM_MIN_SPEED, vspeed));

if (seeking_flag)
{
  solid = false;
}
else
{
  solid = true;
}
</string>
